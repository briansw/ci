      <%= link_to(pdf_or_url(reading), target: '_blank', class: 'reading-button') do %>
        <%= section_bug(reading) %><br /><br />
        <strong><%= reading.title %></strong><br />
        â¤· <%= reading.author %><br />
        <span class="date"><%= reading.year %></span>
      <% end %>
    </div>
  <% end %>
</div>