<!DOCTYPE html>
<html>
<head>
  <title>Core Interaction</title>
  <%= stylesheet_link_tag 'application', media: 'all' %>

  <%= csrf_meta_tags %>
</head>
<body>

  <div id="wrapper">
    <div id="header">
      <%= render 'shared/header' %>
    </div>

    <div id="content">
      <%= yield %>
    </div>
  </div>

  <div id="footer">
    <div id="footer-inner">
      <%= render 'shared/footer' %>
    </div>
  </div>

  <%= render 'shared/wheel_of_fortune' %>
  
  <%= javascript_include_tag 'application' %>
  <%= yield :init_student_list %>

</body>
</html>
